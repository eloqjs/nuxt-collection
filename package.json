{
  "name": "@eloqjs/nuxt-collection",
  "version": "0.0.0",
  "description": "Nuxt module for ELOQJS Collection.",
  "homepage": "https://collection.eloqjs.com",
  "repository": "eloqjs/nuxt-collection",
  "author": "Jo√£o Pedro Antunes Silva",
  "keywords": [
    "collection",
    "collect",
    "eloquent",
    "resources",
    "array",
    "map",
    "filter",
    "laravel"
  ],
  "license": "MIT",
  "publishConfig": {
    "access": "public"
  },
  "main": "build/index.js",
  "types": "types/index.d.ts",
  "files": [
    "build",
    "templates",
    "types"
  ],
  "scripts": {
    "dev": "nuxt-ts example",
    "build": "rimraf ./build && tsc",
    "lint": "eslint --ext .ts,.js,.vue .",
    "lint-fix": "yarn lint --fix",
    "test": "yarn jest"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!src/index.ts",
      "!src/defaults.ts"
    ],
    "coverageDirectory": "coverage",
    "preset": "ts-jest",
    "testEnvironment": "node",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/build/"
    ]
  },
  "dependencies": {
    "@eloqjs/collection": "^1.0.0-alpha.6"
  },
  "devDependencies": {
    "@nuxt/types": "^2.14.7",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/module-test-utils": "^1.6.3",
    "@nuxtjs/tailwindcss": "^3.2.0",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.9",
    "eslint": "^7.14.0",
    "eslint-config-prettier": "^6.15.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "eslint-plugin-jest-formatting": "^2.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-simple-import-sort": "^6.0.1",
    "jest": "^26.6.3",
    "nuxt-edge": "^2.14.8-26768191.9ce75c5d",
    "prettier": "^2.2.0",
    "rimraf": "^3.0.2",
    "semantic-release": "^17.2.4",
    "ts-jest": "^26.4.4",
    "typescript": "^4.1.2"
  }
}
